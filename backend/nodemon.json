{
  "env": {
    "CONNECTION_STRING": "<DATABASE_CONNECTION_STRING>",
    "PORT": 3002,
    "SECRET_KEY": "<ENCRYPTION_SECRET>",
    "USER": "<USER_EMAIL>",
    "PASS": "<USER_PASSWORD>",
    "HOST": "<SERVICE_PROVIDER>",
    "BASE_URL": "<SERVER_BASE_URL>",
    "CORS_ORIGIN_URL":"<CLIENT_BASE_URL>"
  },
  "watch": ["src"],
  "ext": "ts,json",
  "exec": "ts-node ./src/app.ts"
}